{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "ap",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "ap",
            "kebab-case"
        ],
        "one-line": [
            true,
            "check-catch",
            "check-finally",
            "check-else"
        ],
        "curly": [
            true,
            "ignore-same-line"
        ]
    }
}
